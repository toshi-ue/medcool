{
  "_refs": [
    "https://tech.smartshopping.co.jp/composition-api",
    "https://mseeeen.msen.jp/current-nuxt-type-script-at-minimum/",
    "https://zenn.dev/koduki/articles/430ba33bb8b8f6",
    "https://typescript.nuxtjs.org/ja",
    "https://typed-vuex.roe.dev/",
    "https://www.typescriptlang.org/docs/handbook/compiler-options.html",
    "https://vuejs.github.io/vetur/guide/setup.html#vs-code-config"
  ],
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "_TODO: settings": [
      {
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true
      }
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "components/*": [
        "src/components/**/*"
      ]
    },
    "typeRoots": [
      "types",
      "node_modules/@types"
    ],
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/vuetify"
    ]
  },
  "include": [
    ""
  ],
  "exclude": [
    "node_modules",
    ".nuxt",
    "coverage",
    "dist"
  ]
}
